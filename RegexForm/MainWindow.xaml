<Window x:Class="RegexForm.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RegexForm"
        mc:Ignorable="d"
        Title="MainWindow" Height="260" Width="420">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Label Content="Name:" Margin="1,15,325,13"></Label>
        <TextBox x:Name="TextName" Grid.Row="0" Margin="72,18,70,17" Height="20" Width="250"></TextBox>
        <Label Content="Phone:" Margin="1,14,325,12" Grid.Row="1"></Label>
        <TextBox x:Name="TextPhone" Grid.Row="1" Margin="72,18,70,17" Height="20" Width="250"></TextBox>
        <Label Content="Email:" Margin="1,16,325,12" Grid.Row="2"></Label>
        <TextBox x:Name="TextMail" Grid.Row="2" Margin="72,18,70,17" Height="20" Width="250"></TextBox>
        <Button x:Name="SaveBtn" Grid.Row="3" Width="80" Height="20" Margin="242,16,70,19" Click="SaveBtn_Click">Save</Button>
    </Grid>
</Window>
